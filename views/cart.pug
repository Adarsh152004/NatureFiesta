extends base

block content
  .section-cart
    a.cart-back(href="/") < Your Cart
    .section-cart-rows 
      .cart-product 
        p Product
        each product in products 
          p #{product.name}
      .cart-price 
        p Price
        p 1000
        p 1000
      .cart-quantity
        p Quantity
        p 1
        p 1
      .cart-subtotal 
        p Subtotal
        p 1000
        p 1000